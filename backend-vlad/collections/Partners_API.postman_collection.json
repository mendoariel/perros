{
  "info": {
    "name": "Partners API",
    "description": "API para gestionar partners (socios comerciales) que pueden ofrecer artículos, servicios, ofertas y recibir comentarios",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3333",
      "type": "string"
    },
    {
      "key": "authToken",
      "value": "your_jwt_token_here",
      "type": "string"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{authToken}}",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Partners",
      "item": [
        {
          "name": "Create Partner",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Veterinaria Central\",\n  \"address\": \"Av. Principal 123\",\n  \"whatsapp\": \"+1234567890\",\n  \"phone\": \"+1234567890\",\n  \"description\": \"Veterinaria especializada en mascotas\",\n  \"website\": \"https://veterinariacentral.com\",\n  \"partnerType\": \"VETERINARY\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/partners",
              "host": ["{{baseUrl}}"],
              "path": ["partners"]
            }
          }
        },
        {
          "name": "Get All Partners",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/partners",
              "host": ["{{baseUrl}}"],
              "path": ["partners"]
            }
          }
        },
        {
          "name": "Search Partners",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/partners/search?q=veterinaria",
              "host": ["{{baseUrl}}"],
              "path": ["partners", "search"],
              "query": [
                {
                  "key": "q",
                  "value": "veterinaria"
                }
              ]
            }
          }
        },
        {
          "name": "Get Partners by Type",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/partners/type/VETERINARY",
              "host": ["{{baseUrl}}"],
              "path": ["partners", "type", "VETERINARY"]
            }
          }
        },
        {
          "name": "Get Partner by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/partners/1",
              "host": ["{{baseUrl}}"],
              "path": ["partners", "1"]
