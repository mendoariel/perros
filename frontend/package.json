{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll 2000 --port 4100 --disable-host-check",
    "start-ssr": "ng run frontend:serve-ssr --port 4100",
    "start-ssr:staging": "ng run frontend:serve-ssr:staging --port 4101",
    "build": "ng build",
    "build-local": "ng build --configuration development",
    "build-local-deploy": "ng build --configuration local-deploy",
    "build-prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:docker": "ng test --watch=false --browsers=ChromeHeadless",
    "dev:ssr": "ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:ssr": "ng build --configuration production && ng run frontend:server:production",
    "build:ssr:dev": "ng build --configuration development && ng run frontend:server:development",
    "serve:ssr:dev": "ng run frontend:serve-ssr",
    "build:ssr-local-deploy": "ng build --configuration local-deploy && ng run frontend:server:local-deploy",
    "prerender": "ng run frontend:prerender",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:ssr": "eslint src/**/*.ts --config .eslintrc.json --quiet",
    "lint:ssr:fix": "eslint src/**/*.ts --config .eslintrc.json --fix --quiet",
    "lint:ssr:verbose": "eslint src/**/*.ts --config .eslintrc.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/platform-server": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@angular/ssr": "^17.3.17",
    "@auth0/angular-jwt": "^5.2.0",
    "@fortawesome/angular-fontawesome": "^0.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0",
    "ngx-cookie-service": "^17.1.0",
    "ngx-sharebuttons": "^13.0.0",
    "rxjs": "~7.8.0",

    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-eslint/eslint-plugin": "^17.5.3",
    "@angular-eslint/eslint-plugin-template": "^17.5.3",
    "@angular/cli": "~17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.4.5"
  }
}
